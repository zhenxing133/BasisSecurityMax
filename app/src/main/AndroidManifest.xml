<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ithomework.basissecuritymax">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.activity.SettingActivity"/>
        <activity android:name=".activity.activity.LostFindActivity"/>
        <activity android:name=".activity.activity.ActivityOne"/>
        <activity android:name=".activity.activity.ActivityTwo"/>
        <activity android:name=".activity.activity.ActivityThree"/>
        <activity android:name=".activity.activity.ActivityFoure"/>
    </application>

</manifest>